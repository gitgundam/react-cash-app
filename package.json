{
  "name": "react-cash-app",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "commit": "czg emoji"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.33.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "czg": "^1.4.1",
    "eslint": "^8.29.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "echo 'from lint-stage'"
    ]
  }
}